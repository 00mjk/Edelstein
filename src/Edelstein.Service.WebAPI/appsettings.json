{
  "Service": {
    "ID": 0,
    "Name": "WebAPI-0",
    "Host": "localhost",
    "Port": 8080
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
        }
      }
    ]
  },
  "Distribution": {
    "Type": "Redis",
    "ConnectionString": "localhost"
  },
  "Database": {
    "Type": "PostgreSQL",
    "ConnectionString": "host=localhost;database=postgres;"
  },
  "DataParser": {
    "Type": "NX",
    "Path": "./data"
  },
  "Script": {
    "Type": "Lua",
    "Path": "./scripts"
  }
}